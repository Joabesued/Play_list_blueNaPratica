<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>playlist</title>
  </head>
  <body>
    <h1>HEllo world</h1>

    <a href="./admin">Administração</a>
    <section>
      <% playlist.forEach((music)=>{ %>

      <h2><%= music.name %></h2>
      <p><%= music.author %></p>
      <img src="<%= music.linkImage %>" alt="Imagem da musica" />
      <audio controLs src="<%= music.linkImage %>"></audio>

      <% }) %>
    </section>
  </body>
</html>
